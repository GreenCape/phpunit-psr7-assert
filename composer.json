{
    "name": "helmich/phpunit-psr7-assert",
    "description": "PHPUnit assertions for testing PSR7-compliant applications",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Helmich",
            "email": "m.helmich@mittwald.de"
        }
    ],
    "require": {
        "psr/http-message": "^1.0",
        "phpunit/phpunit": ">=4.8",
        "helmich/phpunit-json-assert": "^1.0"
    },
    "require-dev": {
        "guzzlehttp/psr7": "^1.2",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Helmich\\Psr7Assert\\": "src/"
        }
    },
    "autoload-dev": {
        "files": [
            "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php",
            "vendor/helmich/phpunit-json-assert/src/Functions.php",
            "src/Functions.php"
        ]
    }
}
